import math as ma
len=[]
m=0
n=0
a=int(input())
for i in range(0,a):
    j=float(input())
    len.append(j)
    m=m+j
print(len)
mean=m/a
for k in range(0,a):
    n=n+ma.pow((len[i]-mean),2)
vari=n/a
devi=ma.sqrt(vari)
print(f"mean:{mean:.2f}")
print(f"variance:{vari:.2f}")
print(f"deviation:{devi:.2f}")
