l=[]
n=int(input("Enter how many values you want to read:"))
print(n)
temp=0
for i in range(0,n):
    print(f"Enter the value of l[{i}]:",end=" ")
    j=int(input())
    print(j)
    l.append(j)
print(l)
for k in range(0,n):
    for m in range(k+1,n):
        if(l[k]>l[m]):
            temp=l[m]
            l[m]=l[k]
            l[k]=temp
print("Array sorted in ascending order:",l)
